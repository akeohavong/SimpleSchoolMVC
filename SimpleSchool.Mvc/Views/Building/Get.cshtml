@model SimpleSchool.Core.Entities.Building

<h1>Building Information</h1>
<div class="row">
    <div class="col-sm-12 col-md-6 col-lg-4">
        <form>
            <fieldset disabled>
                <div class="mb-3">
                    <label class="form-label" asp-for="BuildingID">Id</label>
                    <input type="text" class="form-control" id="BuildingID" asp-for="BuildingID">
                </div>
                <div class="mb-3">
                    <label asp-for="BuildingName" class="form-label">Name</label>
                    <input type="text" class="form-control" id="BuildingName" asp-for="BuildingName">
                </div>
            </fieldset>
        </form>
    </div>
</div>